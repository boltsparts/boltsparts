author: Andrej <@Andrej730>
blt-version: 0.4
description: Various hotrolled T profiles
id: profile_t
license: LGPL 2.1+ <http://www.gnu.org/licenses/lgpl-2.1>
name: Profiles T
classes:
- id: tbeam_st_imp
  names:
    description: ST profile, imperial US standard t-beam profile
    labeling: '%(type)s, l=%(l)s'
    name: ST Profile
  notes: AISC Shapes Database v15.0, November 2017, https://www.aisc.org/globalassets/aisc/manual/v15.0-shapes-database/aisc-shapes-database-v15.0.xlsx
  parameters:
    defaults:
      l: 12
      type: ST12X60.5
    description:
      b: flange width
      h: beam height
      l: beam length
      r1: root fillet radius
      r2: toe fillet radius
      tw: web thickness
      tf: flange thickness
      type: beam type
    free: [type, l]
    tables:
      columns:  [h, b, tw, tf, r1, r2]
      data: !include profile_t/tbeam_st_imp.yaml
      index: type
    types:
      b: Length (in)
      h: Length (in)
      l: Length (in)
      r1: Length (in)
      r2: Length (in)
      tw: Length (in)
      tf: Length (in)
      type: Table Index
  source: https://www.aisc.org/globalassets/aisc/manual/v15.0-shapes-database/aisc-shapes-database-v15.0.xlsx
  standards:
    body: AISC
    description: ST profile, imperial US standard t-beam profile
    group: AISC 
    labeling: AISC Shapes Database v15.0 %(type)s, l=%(l)s
    standard: AISC Shapes Database v15.0 ST Profile
