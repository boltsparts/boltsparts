      r: root fillet radius
      tf: flange thickness
      tw: web thickness
      type: beam type
    free: [type, l]
    tables:
      columns:  [h, b, tw, tf, r]
      data: !include profile_t/tbeam_mt_imp.yaml
      index: type
    types:
      b: Length (in)
